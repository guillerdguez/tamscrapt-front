{
  "name": "microfrontend-sisbatch",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node dist/microfrontend-sisbatch/server/server.js",
    "build": "ng build --configuration production",
    "build:single-spa": "ng build --configuration production",
    "serve:single-spa": "ng serve --disable-host-check --port 4200 --deploy-url http://localhost:4200/ --live-reload false",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.11",
    "@angular/cdk": "^17.3.8",
    "@angular/common": "^17.3.11",
    "@angular/compiler": "^17.3.11",
    "@angular/core": "17.3.11",
    "@angular/forms": "^19.1.1",
    "@angular/platform-browser": "^17.3.11",
    "@angular/platform-browser-dynamic": "^17.3.11",
    "@angular/router": "^17.3.11",
    "@ngx-formly/bootstrap": "^6.3.12",
    "@ngx-formly/core": "^6.3.12",
    "@ngx-formly/primeng": "^6.0.0",
    "angular-i18next": "17.0.0",
    "core-js": "^3.6.4",
    "i18next": "23.11.5",
    "primeflex": "^3.1.3",
    "primeng": "^17",
    "rxjs": "7.8.1",
    "single-spa": "6.0.1",
    "single-spa-angular": "9.1.2",
    "systemjs-webpack-interop": "^2.3.7",
    "zone.js": "0.14"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.2",
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular/cli": "17.3.8",
    "@angular/compiler-cli": "^17.3.8",
    "@angular/language-service": "^17.3.8",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@ngx-formly/schematics": "6.3.12",
    "@types/jest": "^27.0.2",
    "@types/node": "18.14.6",
    "jest": "29.7.0",
    "jest-preset-angular": "^14.0.1",
    "style-loader": "3.3.1",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "5.2",
    "webpack": "^5",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.1"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "^.+\\.module\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "^config$": "<rootDir>/extra-webpack-config.js"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
